+      MaybeHandle<Object> maybe_value =
+        entry_pair_as_array->GetValue<Object>(isolate, 1);
+      Handle<Object> value;
+      if (!maybe_value.ToHandle(&value)) {
+        value = handle(key->GetHeap()->undefined_value(), isolate);
+      }
