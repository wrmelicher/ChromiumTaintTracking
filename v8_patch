+      CheckType type,
+      SymbolicMemorySlot* memslot) :
+  ExecutionInfo(eval, type),
+  slot_(memslot) {}
+
+VariableLoadExecutionInfo::~VariableLoadExecutionInfo() {}
+
+SymbolicMemorySlot* VariableLoadExecutionInfo::GetSlot() const {
+  return slot_;
