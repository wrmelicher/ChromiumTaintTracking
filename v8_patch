+
+int64_t TaintTracker::Impl::LogToFileImpl(
+    Isolate* isolate,
+    std::unique_ptr<::capnp::MallocMessageBuilder> builder,
+    FlushConfig conf) {
+  if (!IsLogging()) {
     return NO_MESSAGE;
   }
