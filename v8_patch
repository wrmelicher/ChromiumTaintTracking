+std::vector<std::tuple<TaintType, int>> InitTaintRanges(
+    Handle<String> target) {
+  IsTaintedVisitor visitor;
+  {
+    DisallowHeapAllocation no_gc;
+    visitor.run(*target, 0, target->length());
+  }
+  return visitor.GetRanges();
+}
 
