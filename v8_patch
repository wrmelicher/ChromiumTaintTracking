+
+  virtual bool SymbolicExecuteSelf() {
+    return false;
+  }
+
+  virtual std::shared_ptr<SymbolicState> StaticValue() {
+    return saved_literal_;
+  }
+
+private:
+  std::shared_ptr<SymbolicState> saved_literal_;
+};
