+                      v8::Isolate* isolate,
+                      TaintType type) {
+  ENTER_V8(reinterpret_cast<i::Isolate*>(isolate));
+  i::Handle<i::Object> obj = Utils::OpenHandle(*val);
+  tainttracking::SetTaint(obj, type);
+}
