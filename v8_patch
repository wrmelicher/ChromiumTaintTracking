+    default: {
+      VariableLoadExecutionInfo info(
+          branch_condition,
+          check,
+          mem_serializer_.Deserialize(past_label_or_parameter_index));
+      DebugPrintTraceHook(label, info);
+      expr_slot->HandleVariableLoadExecution(info);
+    }
+      break;
+  }
 }
 
 
