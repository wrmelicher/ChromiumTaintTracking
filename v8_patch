+namespace tainttracking {
+enum FrameType {
+  JS,
+  TOP_LEVEL,
+  SETTER_ACCESSOR,
+  GETTER_ACCESSOR,
+
+  // The following types need literal arguments
+
+  // A call to Execution::Call that is not instrumented
+  UNKNOWN_CAPI,
+
+  // Execution::New
+  UNKNOWN_CAPI_NEW,
+
+  // A call to Builtins::InvokeApiFunction
+  UNKNOWN_EXTERNAL,
+
+  FIRST_NEEDS_LITERAL = UNKNOWN_CAPI,
+};
+}
