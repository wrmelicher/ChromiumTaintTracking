-void ObjectOwnPropertiesVisitor::Visit(Handle<JSReceiver> receiver) {
-  ProcessReceiver(receiver);
-  while (!value_stack_.empty()) {
-    Handle<JSReceiver> curr = value_stack_.back();
-    value_stack_.pop_back();
