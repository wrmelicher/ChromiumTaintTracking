 void ConcolicExecutor::OnRuntimeExitFinally() {
+  #ifdef DEBUG
+  if (FLAG_taint_tracking_trace_concolic) {
+    std::cerr << "rethrowing finally current frame stack size "
+              << executing_frames_.size() << std::endl;
+  }
+  #endif
