+template <> void TaintVisitor::VisitIntoStringTemplate<SeqTwoByteString>(
+    SeqTwoByteString* source, int from, int len) {
+  DCHECK_GE(from, 0);
+  DCHECK_GE(len, 0);
+  DCHECK_LE(from + len, source->length());
