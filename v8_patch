     }
   }
+  if (!next_frame->receiver_) {
+    switch (next_frame->type_) {
+      case FrameType::JS:
+        UNREACHABLE();
+        break;
+
+      case FrameType::JS_CALL_NEW: {
+        // TODO: initialize prototype?
+        next_frame->receiver_.reset(
+            new SymbolicMemorySlot(
+                false, SymbolicFactory (owner_->isolate()).OptimizedOut()));
+      }
+        break;
+
+      default: {
+        next_frame->receiver_.reset(
+            new SymbolicMemorySlot(
+                false, SymbolicFactory (owner_->isolate()).OptimizedOut()));
+      }
+        break;
+    }
+  }
   return next_frame;
