+std::shared_ptr<SymbolicState>
+ObjectPropertySymbolicStateManager::GetSymbolicProperties(
+    v8::internal::Handle<v8::internal::JSReceiver> target) {
+  SymbolicObjectPropertyWrapper* prev = Lookup(target);
+  return prev
