+    const NodeLabel& label, const SymbolicMemorySlot& value) {
+  symbolic_memory_[label] = value;
+}
+
+SymbolicMemorySlot ConcolicExecutor::GetMemory(const NodeLabel& label) {
+  return symbolic_memory_[label];
+}
