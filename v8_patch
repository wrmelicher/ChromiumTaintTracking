+static void concolic_api_symbolic_taint(
+    const v8::FunctionCallbackInfo<v8::Value>& info) {
+  v8::TaintTracking::SetSymbolicReturnValueDocumentUrl(info.GetIsolate());
+  info.GetReturnValue().Set(info[0]);
+}
