+template <class T>
+TaintData* StringTaintData_TryAllocate(T* str) {
+  TaintData* answer = StringTaintData(str);
+  if (answer == nullptr) {
+    int len = str->length();
+    answer = new TaintData[len];
+    memset(answer, TaintType::UNTAINTED, len);
+    str->resource()->SetTaintChars(answer);
