+template void OnJoinManyStrings<SeqOneByteString, JSArray>(
+    SeqOneByteString*, JSArray*);
+template void OnJoinManyStrings<SeqTwoByteString, JSArray>(
+    SeqTwoByteString*, JSArray*);
