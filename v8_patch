+void TaintTracker::Impl::MaybeFlush(FlushConfig conf) {
+  unflushed_messages_++;
+  switch(conf) {
+    case FORCE_FLUSH:
+      unflushed_messages_ = 0;
+      Log().flush();
+      break;
+    case LAZY_FLUSH:
+      if (unflushed_messages_ >= kFlushMessageMax) {
         unflushed_messages_ = 0;
         Log().flush();
-        break;
