+      unallocated @1;
+      context @2;
+      global @3;
+      lookupSlot @4;
+      local @5;
+    }
+
+    scope @0 :ScopePointer;
+    name @1 :JsString;
+    mode @2 :VariableMode;
+    kind @3 :Kind;
+    initializationFlag @4 :InitializationFlag;
+    location @5 :Location;
+  }
+
+  # Begin node types
+
