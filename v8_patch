+template <typename Val>
+Status ConcolicExecutor::Lookup(
+    const NodeLabel& label, Val* out,
+    std::unordered_map<NodeLabel,
+                       Val,
+                       NodeLabel::Hash,
+                       NodeLabel::EqualTo>& vals) {
+  if (!label.IsValid()) {
+    return Status::FAILURE;
+  }
+  auto contains = vals.find(label);
+  if (contains == vals.end()) {
+    return Status::FAILURE;
+  }
+  *out = contains->second;
+  return Status::OK;
+}
+
