+    iterator @1 :Variable;
+    assignIterator @2 :Node;
+    nextResult @3 :Node;
+    resultDone @4 :Node;
+    assignEach @5 :Node;
+  }
+
+  struct WithStatement {
+    scope @0 :ScopePointer;
+    expression @1 :Node;
+  }
+
+  struct TryCatchStatement {
+    scope @0 :ScopePointer;
+    variable @1 :Variable;
+    catchBlock @2 :Block;
+    tryBlock @3 :Block;
+  }
+
