+#include <capnp/message.h>
+#include <capnp/serialize.h>
+
+#include <v8/ast.capnp.h>
+
+#include "src/ast/ast.h"
+
+namespace tainttracking {
+  bool SerializeAst(v8::internal::FunctionLiteral* ast,
+                    ::Ast::Builder* message,
+                    v8::internal::Isolate* isolate);
+}
